<!-- 单个任务详情 -->
<div class="modal fade" id="modal-default">
    <div class="modal-dialog">
        <div class="modal-content">
            <div class="modal-header">
                <button type="button" class="close" data-dismiss="modal" aria-label="Close">
                    <span aria-hidden="true">&times;</span>
                </button>
                <h4 class="modal-title">添加教学任务</h4>
            </div>
            <div class="modal-body">
                <div class="box box-warning">
                    <div class="box-body">
                        <form role="form">
                            <div class="form-group">
                                <label>标题</label>
                                <input type="text" class="form-control" placeholder="Enter ..." id="title">
                            </div>
                            <div class="form-group">
                                <label>及格线</label>
                                <input type="number" class="form-control" placeholder="Enter ..." id="pass">
                            </div>
                            <div class="form-group">
                                <label>分数</label>
                                <input type="number" class="form-control" placeholder="Enter ..." id="value">
                            </div>
                            <div class="form-group">
                                <label>距离(米)</label>
                                <input type="number" class="form-control" placeholder="Enter ..." id="distance">
                            </div>
                            <div class="form-group">
                                <label>描述</label>
                                <textarea class="form-control" rows="3" placeholder="Enter ..." id="desc"></textarea>
                            </div>

                            <div class="form-group">
                                <div class="radio">
                                    <label>
                                        <input type="radio" name="optionsRadios" id="optionsRadios1" value="option1" checked=""> 跑步
                                    </label>
                                </div>
                                <div class="radio">
                                    <label>
                                        <input type="radio" name="optionsRadios" id="optionsRadios2" value="option2"> 游泳
                                    </label>
                                </div>
                            </div>

                            <div class="form-group">
                                <label>Date and time range:</label>

                                <div class="input-group">
                                    <div class="input-group-addon">
                                        <i class="fa fa-clock-o"></i>
                                    </div>
                                    <input type="text" class="form-control pull-right" id="reservationtime">
                                </div>
                            </div>

                            <div class="form-group">
                                <label>接受人员</label>
                                <select class="form-control" id="receiver">
                                    <option uid='all'>所有人</option>
                                </select>
                            </div>


                        </form>
                    </div>
                </div>
            </div>
            <div class="modal-footer">
                <button type="button" class="btn btn-default pull-left" data-dismiss="modal">Close</button>
                <button type="button" class="btn btn-primary" id="sendinfo">Ok</button>
            </div>
        </div>
    </div>
</div>
